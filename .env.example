# Example Environment Variables
# Bu dosyayý kopyalayýp .env olarak yeniden adlandýrýn
# Kendi deðerlerinizi girin

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME=Todo App
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Authentication
VITE_TOKEN_STORAGE_KEY=todo_app_token
VITE_REFRESH_TOKEN_STORAGE_KEY=todo_app_refresh_token

# Features Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# External Services (örnek)
# VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
# VITE_FIREBASE_API_KEY=your_firebase_key_here
